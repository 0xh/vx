{
  "name": "@vx/text",
  "version": "0.0.150",
  "description": "vx text",
  "main": "index.js",
  "module": "es/vx-text.production.js",
  "jsnext:main": "es/vx-text.production.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "build": "npm run clean && npm run build:umd && npm run build:umd:min && npm run build:es && npm run build:es:min",
    "build:umd": "NODE_ENV=development FORMAT=umd rollup -c ./rollup.config.js",
    "build:umd:min": "NODE_ENV=production FORMAT=umd rollup -c ./rollup.config.js",
    "build:es": "NODE_ENV=development FORMAT=es rollup -c ./rollup.config.js",
    "build:es:min": "NODE_ENV=production FORMAT=es rollup -c ./rollup.config.js",
    "clean": "rimraf ./umd ./es",
    "prepare": "npm run build"
  },
  "files": [
    "es/",
    "umd/",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hshoff/vx.git"
  },
  "keywords": [
    "vx",
    "react",
    "d3",
    "visualization",
    "chart"
  ],
  "author": "@techniq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hshoff/vx/issues"
  },
  "homepage": "https://github.com/hshoff/vx#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "reduce-css-calc": "^1.3.0"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "colors": "^1.1.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "jest": "^21.2.1",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0"
  },
