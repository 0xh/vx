{
  "env": {
    "test": {
      "presets": ["env", "stage-0", "react"],
      "plugins": []
    },
    "development": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": { "browsers": ["> 1%", "last 2 versions", "IE 10"] }
          }
        ],
        "stage-0",
        "react"
      ],
      "plugins": ["external-helpers"]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": { "browsers": ["> 1%", "last 2 versions", "IE 10"] }
          }
        ],
        "stage-0",
        "react"
      ],
      "plugins": ["external-helpers"]
    }
  }
}
