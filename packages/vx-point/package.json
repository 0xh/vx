{
  "name": "@vx/point",
  "version": "0.0.143",
  "description": "vx point",
  "main": "index.js",
  "repository": "https://github.com/hshoff/vx",
  "module": "es/vx-point.production.min.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "build": "npm run clean && npm run build:umd && npm run build:umd:min && npm run build:es && npm run build:es:min",
    "build:umd": "NODE_ENV=development FORMAT=umd rollup -c ./rollup.config.js",
    "build:umd:min": "NODE_ENV=production FORMAT=umd rollup -c ./rollup.config.js",
    "build:es": "NODE_ENV=development FORMAT=es rollup -c ./rollup.config.js",
    "build:es:min": "NODE_ENV=production FORMAT=es rollup -c ./rollup.config.js",
    "clean": "rimraf ./umd ./es",
    "prepublish": "npm run build"
  },
  "files": [
    "es/",
    "umd/",
    "index.js"
  ],
  "keywords": [
    "vx",
    "data",
    "visualizations",
    "charts"
  ],
  "author": "@hshoff",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "jest": "^21.2.1",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/enzyme-setup.js"
    ]
  }
}
