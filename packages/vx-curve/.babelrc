{
  "env": {
    "test": {
      "presets": ["stage-0", "env"],
      "plugins": ["transform-react-jsx"]
    },
    "development": {
      "presets": [
        "stage-0",
        [
          "env",
          {
            "modules": false,
            "targets": { "browsers": ["> 1%", "last 2 versions", "IE 10"] }
          }
        ]
      ],
      "plugins": ["external-helpers", "transform-react-jsx"]
    },
    "production": {
      "presets": [
        "stage-0",
        [
          "env",
          {
            "modules": false,
            "targets": { "browsers": ["> 1%", "last 2 versions", "IE 10"] }
          }
        ]
      ],
      "plugins": ["external-helpers", "transform-react-jsx"]
    }
  }
}
