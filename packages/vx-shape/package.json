{
  "name": "@vx/shape",
  "version": "0.0.147",
  "description": "vx shape",
  "repository": "https://github.com/hshoff/vx",
  "main": "index.js",
  "module": "es/vx-shape.production.min.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "build": "npm run clean && npm run build:umd && npm run build:umd:min && npm run build:es && npm run build:es:min",
    "build:umd": "NODE_ENV=development FORMAT=umd rollup -c ./rollup.config.js",
    "build:umd:min": "NODE_ENV=production FORMAT=umd rollup -c ./rollup.config.js",
    "build:es": "NODE_ENV=development FORMAT=es rollup -c ./rollup.config.js",
    "build:es:min": "NODE_ENV=production FORMAT=es rollup -c ./rollup.config.js",
    "clean": "rimraf ./umd ./es",
    "prepublish": "npm run build"
  },
  "files": ["es/", "umd/", "index.js"],
  "keywords": ["vx", "react", "d3", "visualizations", "charts"],
  "author": "@hshoff",
  "license": "MIT",
  "dependencies": {
    "@vx/curve": "0.0.143",
    "@vx/group": "0.0.143",
    "@vx/point": "0.0.143",
    "classnames": "^2.2.5",
    "d3-shape": "^1.2.0",
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "d3-array": "^1.2.0",
    "d3-hierarchy": "^1.1.5",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "jest": "^21.2.1",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "setupFiles": ["raf/polyfill", "<rootDir>/test/enzyme-setup.js"]
  }
